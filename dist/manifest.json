{
  "manifest_version": 3,
  "name": "Calendar Survey Tool",
  "version": "1.0",
  "oauth2": {
    "client_id": "585748688541-2q572p30qujhlgb681gv12s8c41fjii9.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/forms.body",
      "https://www.googleapis.com/auth/calendar.events",
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/drive.file"
    ]
  },
  "permissions": [
    "identity",
    "storage",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "https://*.googleapis.com/*"
  ],
  "action": {
    "default_popup": "src/popup.html"
  },
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "assets/content.ts-loader-AJBAAtfZ.js"
      ],
      "matches": [
        "https://calendar.google.com/calendar/*/eventedit/*"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "https://calendar.google.com/*"
      ],
      "resources": [
        "assets/content.ts-B2ul3Qfb.js"
      ],
      "use_dynamic_url": true
    }
  ]
}
